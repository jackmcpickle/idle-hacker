{
    "name": "idle-hacker",
    "main": "./src/worker/index.ts",
    "compatibility_date": "2026-01-19",
    "compatibility_flags": ["nodejs_compat"],
    "vars": {
        "TURSO_DATABASE_URL": "libsql://idle-hacker-game-jackmcpickle.turso.io"
    },
    "observability": {
        "enabled": false,
        "head_sampling_rate": 1,
        "logs": {
            "enabled": true,
            "head_sampling_rate": 1,
            "persist": true,
            "invocation_logs": true
        },
        "traces": {
            "enabled": true,
            "persist": true,
            "head_sampling_rate": 1
        }
    }
}
